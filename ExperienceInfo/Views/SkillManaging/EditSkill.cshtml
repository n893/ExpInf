@model ExperienceInfo.Models.SkillModel

@{
    ViewBag.Title = "EditSkill";
}

<h2>Edit skill</h2>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>CategoryModel</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.SkillName)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.SkillName)
            @Html.ValidationMessageFor(model => model.SkillName)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}